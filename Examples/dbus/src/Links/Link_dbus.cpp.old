#include "Links/Link_dbus.hpp"

Dbus_adapter &Link_dbus_in::get_caller(std::string p_key) {
  return *this->a_c[p_key];
}

bool Link_dbus_in::connected(std::string p_key) {
  return this->a_c.find(p_key) != this->a_c.end();
}

void Link_dbus_in::set_in(std::string p_key, Interface *to) {
  this->a_c[p_key] = to->get_dbus_adapter();
}