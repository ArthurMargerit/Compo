#include "Ports/CompoMe/Stream/map_in.hpp"
#include "CompoMe/Log.hpp"

namespace CompoMe {

namespace Stream {

//! Default constructor
map_in::map_in() {}

//! Destructor
map_in::~map_in() {}

// Function IN ////////////////////////////////////////////////////////////////
bool map_in::connect_interface(CompoMe::String interface,
                               CompoMe::Interface &p_i) {
  if (p_i.get_caller_stream() == nullptr) {
    C_INFO_TAG("port,stream",
               "CallerStream is not available for this interface");
    return false;
  }

  return CompoMe::Core::map_in::connect_interface(interface, p_i);
}

bool map_in::is_connected_interface(CompoMe::String interface) {
  return CompoMe::Core::map_in::is_connected_interface(interface);
}

bool map_in::is_connected_interface(CompoMe::Interface &p_i) {
  return CompoMe::Core::map_in::is_connected_interface(p_i);
}

bool map_in::disconnect_interface(CompoMe::String interface) {
  return CompoMe::Core::map_in::disconnected_interface(interface);
}

bool map_in::disconnect_interface(CompoMe::Interface &p_i) {
  return CompoMe::Core::map_in::disconnected_interface(p_i);
}

CompoMe::Interface &map_in::get_interface(CompoMe::String interface) {
  return CompoMe::Core::map_in::get_interface(interface);
}

std::map<std::tuple<CompoMe::String>, CompoMe::Interface *>
map_in::get_interfaces_list() {
  return CompoMe::Core::map_in::get_interface_list();
}

} // namespace Stream

} // namespace CompoMe
