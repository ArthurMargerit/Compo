digraph{
node [shape=record];
{% for c in INSTANCE %}
{{c.NAME}} [label="{ {% for b in c.COMPONENT.PROVIDE %}<{{b.NAME}}>{{b.NAME}}{%if not loop.last%}|{%endif%}{% endfor %} } | { {{c.NAME}} {{c.COMPONENT.NAME}} | } |{ {% for b in c.COMPONENT.REQUIRE %}<{{b.NAME}}>{{b.NAME}}{%if not loop.last%}|{%endif%}{% endfor %} }"]
{% endfor %}

{% for l in CONNECTION %}
   {{l.FROM.INSTANCE.NAME}}:{{l.FROM.INTERFACE.NAME}} -> {{l.TO.INSTANCE.NAME}}:{{l.TO.INTERFACE.NAME}};
{% endfor %}

}