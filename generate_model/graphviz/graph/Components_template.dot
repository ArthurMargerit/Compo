<table>
  <tr>
    <td>{{NAME}}</td>
    <td colspan="2"> {{ '{{' }} NAME{{ '}}' }} </td>
  </tr>
  <tr>
    <td>
      <table>
        <tr>
          <td colspan="3"> PROVIDE</td>
        </tr>

        {% for b in PROVIDE %}
        <tr>
          <td PORT="{{b.NAME}}">X</td>
          <td>{{b.INTERFACE.NAME}}</td>
          <td>{{b.NAME}}</td>
        </tr>
        {% endfor %}
        
      </table>
    </td>
    <td>
      <table>
        <tr>
          <td colspan="3">DATA</td>
        </tr>
        
        {% for b in DATA %}
        <tr>
          <td>{{b.TYPE.NAME}}</td>
          <td>{{b.NAME}}</td>
          <td>{{b.VALUE}}</td>
          <td>= {{b.DEFAULT}}</td>
        </tr>
        {% endfor %}
        
      </table>
    </td>
    
    <td>
      <table>
        
        <tr>
          <td colspan="3">REQUIRE</td>
        </tr>
        
        
        {% for b in REQUIRE %}
        <tr>
          <td>{{b.INTERFACE.NAME}}</td>
          <td>{{b.NAME}}</td>
          <td PORT="{{b.NAME}}">X</td>
        </tr>
        {% endfor %}
        
      </table>
    </td>
  </tr>
  
</table>

